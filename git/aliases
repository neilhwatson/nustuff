# vim: set ft=gitconfig
[alias]

   # nb = "!f(){ git checkout -b \"$1\"; }; f"
   aliases = config --get-regexp alias
   ci = commit
   cp = "!f(){ git commit --all --message \"$1\" && git push; }; f"
   db = "!f(){ git push -d origin \"$1\"; git branch -d \"$1\" }; f"
   last = log -1 HEAD
   mr = push -u origin HEAD
   nb = switch -c
   nope = reset --hard HEAD
   oops = commit --amend
   re = restore
   st = status
   sw = switch


